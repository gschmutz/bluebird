{
  "name": "@sdairs/bluebird",
  "version": "1.0.12",
  "description": "Bluebird CLI for streaming data to various data services",
  "type": "module",
  "exports": "./dist/index.js",
  "bin":  "./bin/run",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "pnpm run clean && tsc && shx cp bin/run.js bin/run",
    "clean": "shx rm -rf dist",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "pnpm run build && oclif manifest && shx cp bin/run.js bin/run",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.7",
    "@oclif/core": "^3.15.1",
    "@skyware/jetstream": "^0.2.1",
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "oclif": "^4.1.0",
    "shx": "^0.3.4",
    "typescript": "^5.3.3"
  },
  "oclif": {
    "bin": "bluebird",
    "dirname": "bluebird",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " ",
    "topics": {
      "start": {
        "description": "Start the bluebird feed"
      }
    }
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "packageManager": "pnpm@9.15.0"
}